{
  "name": "scenario-ts",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run clean && tsc --build",
    "clean": "rimraf dist",
    "test": "DEBUG=scenario-ts:* jest",
