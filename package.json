    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "debug": "^4.3.0",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.27.0",
    "@eslint/config-helpers": "0.2.1",
    "@eslint/js": "9.25.1",
    "@types/debug": "^4.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.25.1",
    "eslint-plugin-jest": "28.11.0",
    "globals": "16.0.0",
    "jest": "^29.5.0",
    "jsonc-eslint-parser": "2.4.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "stylistic": "0.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "8.31.1"
  }
}
